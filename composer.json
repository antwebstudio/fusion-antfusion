{
    "name": "antweb/fusion-antfusion",
    "description": "A FusionCMS addon.",
    "keywords": [
        "laravel",
        "fusioncms"
    ],
    "license": "MIT",
    "require": {
        "maatwebsite/excel": "*",
        "fusioncms/cms": "*",
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Addons\\AntFusion\\": "src/",
            "AntFusion\\": "src/"
        }
    },
    "extra": {
        "fusioncms": {
            "name": "AntFusion"
        },
        "laravel": {
            "providers": [
                "Addons\\AntFusion\\Providers\\AddonServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
